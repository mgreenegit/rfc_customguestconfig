{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "adminUsername": {
        "reference": {
            "keyVault": {
              "id": "/subscriptions/$(subscriptionId)/resourceGroups/ContosoDev/providers/Microsoft.KeyVault/vaults/vmusers"
            },
            "secretName": "adminUsername"
          }
      },
      "adminPassword": {
        "reference": {
          "keyVault": {
            "id": "/subscriptions/$(subscriptionId)/resourceGroups/resourceGroupName/providers/Microsoft.KeyVault/vaults/vaultName"
          },
          "secretName": "adminPassword"
        }
      }
    }
  }